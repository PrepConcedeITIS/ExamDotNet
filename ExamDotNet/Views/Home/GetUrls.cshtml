
@{
    ViewData["Title"] = "GetUrls";

}
@model List<string>
<h1>Search Results</h1>
@if (Model.Count != 0)
{
    @foreach (var link in Model)
    {
        <p>@link</p>
    }
    <form method="post" asp-action="SaveToDb">
        <input type="hidden" value="@Newtonsoft.Json.JsonConvert.SerializeObject(Model)" name="links"/>
        <button type="submit">Save to DB</button>
    </form>
}
else { 
    <p>Unable to reach site</p>
}